<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="456.0" prefWidth="604.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controller.LoginController">
   <children>
      <VBox alignment="TOP_CENTER" layoutX="189.0" layoutY="150.0" prefHeight="83.0" prefWidth="254.0" spacing="10.0">
         <children>
            <TextField fx:id="fieldUsername" alignment="TOP_CENTER">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </TextField>
            <PasswordField fx:id="fieldPassword" alignment="TOP_CENTER" prefHeight="31.0" prefWidth="165.0">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </PasswordField>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
      <Label layoutX="111.0" layoutY="159.0" text="Username" />
      <Label layoutX="113.0" layoutY="196.0" text="Password" />
      <Button fx:id="btnLogIn" contentDisplay="CENTER" layoutX="189.0" layoutY="244.0" mnemonicParsing="false" onAction="#btnLogInAction" prefHeight="31.0" prefWidth="73.0" text="Sign in" />
      <Button fx:id="btnSave" contentDisplay="CENTER" layoutX="279.0" layoutY="244.0" mnemonicParsing="false" onAction="#btnSaveAction" prefHeight="31.0" prefWidth="73.0" text="Sign up" />
      <Button fx:id="btnClear" layoutX="370.0" layoutY="244.0" mnemonicParsing="false" onAction="#btnClearAction" prefHeight="25.0" prefWidth="73.0" text="Clear" />
      <Label fx:id="labelError" alignment="CENTER" contentDisplay="CENTER" layoutX="193.0" layoutY="288.0" prefHeight="17.0" prefWidth="246.0" textAlignment="CENTER" textFill="#da0000">
         <font>
            <Font size="10.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
